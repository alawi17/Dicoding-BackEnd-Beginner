{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off"
    },
    "eqeqeq": [2, "smart"] //menghilangkan error pada eslint ketika menggunakan == daripada ===
}
